FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      combustionProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

combustionModel flameFoam;

flameFoamCoeffs
{
    Tu false;
    debug false;

   X_H2_0    0.0152;  // Mass fraction for H2
X_O2_0    0.2315;  // Mass fraction for O2
X_N2_0    0.7533;  // Mass fraction for N2 (balance)

Y_H2_99    0.0626;  // Mass fraction for H2
Y_O2_99    0;  // Mass fraction for O2
Y_N2_99    0;  // Mass fraction for N2 (balance)

    reactionRate DDT;
    laminarBurningVelocity hydrogenAirPower;
    wrinklingFactor wFTransport;
    autoIgnition aITransport;

    H0 1.1996e8;

    Le 1.48;
}


